<!DOCTYPE html>
<html>
<head>
  <title>Advantages</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= javascript_pack_tag 'frontend/application' %>
  <%= stylesheet_pack_tag 'frontend/application' %>

</head>
<body>
  <div id='<%= @page_class %>' class="my-container container-fluid g-0">
    <div class="main">
      <%= render 'advantage_header' %>

      <%= yield %>
      <% if request.path != advantages_path %>
        <%= render 'advantage_bottom' %>
      <% end %>

    </div>

  </div>

</body>
</html>

<div class=" position-relative window-holder" style="" >
  <div class="business-detail-bg-img w-100 h-100 position-absolute">
    <%= image_pack_tag 'media/images/businesses/electronic/main-bg-computer.png', class: '', style: '' %>
    <%= image_pack_tag 'media/images/businesses/electronic/main-bg-phone.png', class: '', style: '' %>
    <%= image_pack_tag 'media/images/businesses/electronic/main-bg-etc.png', class: '', style: '' %>
  </div>

  <div class=" row g-0 position-relative">

    <!-- Slider main container -->
    <div class="swiper-container businesses-main-swiper position-relative" style=" background-color: rgba(0,0,0,0.6); ">
      <!-- Additional required wrapper -->
      <div class="swiper-wrapper">
        <!-- Slides train slide -->
        <div class="swiper-slide position-relative">
          <div class="w-100" style="height: 187px;"></div>
          <!-- text intro -->
          <div class="position-absolute" style="top:225px; left: 60px;">
            <%= image_pack_tag 'media/images/businesses/electronic/text.png', class: '', style: '' %>
          </div>

          <!-- product container -->
          <div class=" position-relative"  style="width: 1584px; height: 507px; margin-left: 172px; background: no-repeat left top url(<%= asset_pack_path 'media/images/businesses/electronic/computer.png'%>) " >
            <!-- point 1 -->
            <% points = [
              { container_style: "top:112px; left:735px;", arrow_container_style: "top: 247px; left: 202px;" },
              { container_style: "top:37px; left:1147px;", arrow_container_style: "top: 255px; left: 75px;" },
              { container_style: "top:367px; left:1372px; ", arrow_container_style: "top: -30px; left: 82px; z-index:1;" },
            ] %>
            <% points.each_with_index do |point, i| %>
              <% humanId = i += 1 %>
              <div class="component-point-container position-absolute" data-type='computer' data-id=<%= humanId %> style='<%= point[:container_style] %>' >
                <div class="position-relative">
                  <%= image_pack_tag "media/images/businesses/electronic/computer/titles/#{humanId}.png", class: 'title-img ', style: 'left: 0; top:0;' %>
                  <%= image_pack_tag "media/images/businesses/electronic/computer/titles/#{humanId}-active.png", class: 'title-img-active ', 'data-id': humanId, style: 'left: 0; top:0;' %>

                  <%= link_to( "#{businesses_electronic_details_path}?slide=0&id=#{humanId}" ) do %>
                    <div class="arrow-container animate__animated animate__fadeInDown" style='<%= point[:arrow_container_style] %>' >
                      <div class="position-relative">
                        <div class="arrow-2">
                        </div>
                        <div class="arrow-1 animate__animated animate__infinite zoomIn"></div>
                      </div>
                    </div>
                  <% end %>

                </div>
              </div>
            <% end %>
          </div>

        </div>
        <!-- slide 2 oil-truck-slide -->
        <div class="swiper-slide position-relative">
          <div class="w-100" style="height: 277px;"></div>
          <!-- text intro -->
          <div class="position-absolute" style="top:225px; left: 60px;">
            <%= image_pack_tag 'media/images/businesses/electronic/text.png', class: '', style: '' %>
          </div>

          <!-- product container -->
          <div class=" position-relative"  style="width: 461px; height: 420px; margin-left: 1155px; background: no-repeat left top url(<%= asset_pack_path 'media/images/businesses/electronic/phone.png'%>) " >
            <!-- point 1 -->
            <% points = [
              { container_style: "top:-82px; left:-172px;", arrow_container_style: "top: 255px; left: 228px;" },
              { container_style: "top:285px; left:-142px;", arrow_container_style: "top: -37px; left: 307px;" },
              { container_style: "top:-82px; left:262px;", arrow_container_style: "top: 127px; left: -30px;" },
              { container_style: "top:225px; left:262px;", arrow_container_style: "top: -30px; left: -30px;" },
            ] %>
            <% points.each_with_index do |point, i| %>
              <% humanId = i += 1 %>
              <div class="component-point-container position-absolute" data-type='phone' data-id=<%= humanId %> style='<%= point[:container_style] %>' >
                <div class="position-relative">
                  <%= image_pack_tag "media/images/businesses/electronic/phone/titles/#{humanId}.png", class: 'title-img ', style: 'left: 0; top:0;' %>
                  <%= image_pack_tag "media/images/businesses/electronic/phone/titles/#{humanId}-active.png", class: 'title-img-active ', 'data-id': humanId, style: 'left: 0; top:0;' %>

                  <%= link_to( "#{businesses_electronic_details_path}?slide=1&id=#{humanId}" ) do %>
                    <div class="arrow-container animate__animated animate__fadeInDown" style='<%= point[:arrow_container_style] %>' >
                      <div class="position-relative">
                        <div class="arrow-2">
                        </div>
                        <div class="arrow-1 animate__animated animate__infinite zoomIn"></div>
                      </div>
                    </div>
                  <% end %>

                </div>
              </div>
            <% end %>
          </div>

        </div>

        <!-- slide 3 bus-slide -->
        <div class="swiper-slide position-relative">
          <div class="w-100" style="height: 262px;"></div>
          <!-- text intro -->
          <div class="position-absolute" style="top:225px; left: 60px;">
            <%= image_pack_tag 'media/images/businesses/electronic/text.png', class: '', style: '' %>
          </div>

          <!-- product container -->
          <div class=" position-relative"  style="width: 1332px; height: 472px; margin-left: 600px; background: no-repeat left top url(<%= asset_pack_path 'media/images/businesses/electronic/etc.png'%>) " >
            <!-- point 1 -->
            <% points = [
              { container_style: "top:150px; left:360px;", arrow_container_style: "top: 165px; left: 112px;" },
              { container_style: "top:-22px; left:787px;", arrow_container_style: "top: 217px; left: 22px;" },
            ] %>
            <% points.each_with_index do |point, i| %>
              <% humanId = i += 1 %>
              <div class="component-point-container position-absolute" data-type='sky-train' data-id=<%= humanId %> style='<%= point[:container_style] %>' >
                <div class="position-relative">
                  <%= image_pack_tag "media/images/businesses/electronic/etc/titles/#{humanId}.png", class: 'title-img ', style: 'left: 0; top:0;' %>
                  <%= image_pack_tag "media/images/businesses/electronic/etc/titles/#{humanId}-active.png", class: 'title-img-active ', 'data-id': humanId, style: 'left: 0; top:0;' %>

                  <%= link_to( "#{businesses_electronic_details_path}?slide=2&id=#{humanId}" ) do %>
                    <div class="arrow-container animate__animated animate__fadeInDown" style='<%= point[:arrow_container_style] %>' >
                      <div class="position-relative">
                        <div class="arrow-2">
                        </div>
                        <div class="arrow-1 animate__animated animate__infinite zoomIn"></div>
                      </div>
                    </div>
                  <% end %>

                </div>
              </div>
            <% end %>
          </div>

        </div>


      </div>

      <div class="my-swiper-navigation-container">
        <div class="my-swiper-button-prev"></div>
        <div class="my-swiper-button-next"></div>
      </div>

      <!-- If we need pagination -->
      <div class="swiper-pagination business-swiper-pagination">
        <div class="my-swiper-pagination-bullet">
        </div>
      </div>
      <!-- position absolute, bottom center, a little bit right. -->
      <div class="title-pagination position-absolute" style="z-index: 1020; left: 937px; bottom: 52px;">
        <!-- title 0, data-id="0", on click main swiper move to data-id -->
        <!-- title 1 -->
        <div class="row g-0">
          <div class="col business-swiper-tag position-relative" data-id="1" >
            <%= image_pack_tag 'media/images/businesses/electronic/1-tag.png' %>
            <%= image_pack_tag 'media/images/businesses/electronic/1-tag-active.png', class: 'active-img position-absolute d-none', style: "top:0; left:0;" %>
          </div>
          <div class="col business-swiper-tag ms-5 position-relative" data-id="2" >
            <%= image_pack_tag 'media/images/businesses/electronic/2-tag.png' %>
            <%= image_pack_tag 'media/images/businesses/electronic/2-tag-active.png', class: 'active-img position-absolute d-none', style: "top:0; left:0;" %>
          </div>
        </div>

        <div class="row g-0 mt-5">
          <div class="col business-swiper-tag position-relative" data-id="3" >
            <%= image_pack_tag 'media/images/businesses/electronic/3-tag.png' %>
            <%= image_pack_tag 'media/images/businesses/electronic/3-tag-active.png', class: 'active-img position-absolute d-none', style: "top:0; left:0;" %>
          </div>
        </div>
      </div>

    </div>

    <div class="position-relative"  style="width:47%;">
      <%# render 'navs', return_path: parties_path %>

    </div>

  </div>






</div>





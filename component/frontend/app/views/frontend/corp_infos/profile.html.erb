<main class="window-holder position-relative" style="background-color: #0044ff;
background-image: linear-gradient(135deg, #0044ff 0%, #006eff 50%, #00edff 100%);
"  >
  <div class="w-100" style="height: 220px;" >
  </div>
  <h1>Profile</h1>

  <div class="row g-0">
    <div class="" style="width: 80%; ">
      <video
          id="auto-video"
          class="video-js my-vjs-skin"
          controls
          preload="auto"
          data-setup="{}">
        <source src="/videos/mad.mp4" type="video/mp4"></source>
      </video>
    </div>

    <div class="" style="width: 20%;">
      <%= render 'corp_right_links' %>

      <div class="row g-0 justify-content-end text-blue-500 fw-bold" style=" line-height: 1; font-size: 4rem">
        <div class="col-12 text-end" style="">
          CORPORATE
        </div>
        <div class="col-12 text-end" style="">
          INTRODUCTION
        </div>
        <div class="col-12 text-end" style="font-size: 5rem">
          企业概况
        </div>
      </div>

    </div>
  </div>


  <div class="position-fixed w-100" style="bottom: 0">
    <%= image_pack_tag 'media/images/corp/profile-bottom.png', class: 'w-100' %>
  </div>
</main>

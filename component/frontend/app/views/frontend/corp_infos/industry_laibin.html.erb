<main class="window-holder position-relative"   >

  <div class="w-100" style="height: 22%;" ></div>

  <!-- align-items-center justify-content-center -->
  <div class="row g-0 justify-content-center m-auto position-relative" style="width:95%; ">
    <!-- left -->
    <div class="col text-startCount">
      <%= image_pack_tag "media/images/corp/industry/laibin/main-title.png", class: '', style: '' %>
    </div>

    <!-- right -->
    <div class="col">
      <div class="w-100" style="height: 180px;" ></div>

      <div class="row g-0 w-100 d-flex justify-content-end">
        <div class="col-auto position-relative" style="width: 873px; height:56px;">
          <%= image_pack_tag "media/images/corp/industry/laibin/title.png", class: 'position-absolute', style: 'top:0; left: -90px;' %>
        </div>
        <div class="w-100" style="" ></div>

        <div class="d-flex justify-content-center align-items-center" style="width: 873px; height: 274px;background: center no-repeat url(<%= asset_pack_path 'media/images/corp/industry/laibin/text-bg.png'%>)">
          <%= image_pack_tag "media/images/corp/industry/laibin/text.png" %>

        </div>
      </div>


    </div>

  </div>


  <%= render 'profile_bottom', return_path: corp_infos_industry_path %>

</main>

<main class="window-holder position-relative" style="background: url(<%= asset_pack_path 'media/images/corp/strategy/bg.png'%>)"  >

  <div id="strategy-video-wrap" class="position-absolute w-100 h-100 d-none video-bg-dark" style="top:0; left:0; z-index:10;">
    <div class="row g-0 h-100 align-items-center justify-content-center position-relative">
      <div class="col-auto position-relative" style="margin-top: -80px; ">
        <!-- <i class="bi bi-x-lg position-absolute close-video" style="font-size: 2rem; color: white; bottom: 0; left: -2em;"></i> -->
        <!-- data-setup='{"controls": true}'> -->
        <!-- vjs-show-big-play-button-on-pause -->
        <video
            id="strategy-video"
            class="video-js my-vjs-skin "
            width="1550"
            enableTouchActivity
            data-setup=''>
          <source src="/videos/strategy.mp4" type="video/mp4"></source>
          <p class="vjs-no-js">
            To view this video please enable JavaScript, and consider upgrading to a
            web browser that
          </p>
        </video>
      </div>
    </div>
  </div>

  <div class="w-100" style="height: 180px;" >
  </div>

  <!-- align-items-center justify-content-center -->
  <div class="row g-0 justify-content-center m-auto" style="width:90%; ">
    <div class="row g-0 " >
      <div class="w-100 " style="height: 100px;"></div>
      <div class="col-6 text-center position-relative my-sequence" data-sequence-id='1' data-movement='slideInLeft' style=""  >
        <%= image_pack_tag 'media/images/corp/strategy/xcl/left.png' %>
      </div>

      <div class="col-6 text-center position-relative my-sequence" data-sequence-id='1' data-movement='slideInRight' style=""  >
        <%= image_pack_tag 'media/images/corp/strategy/xcl/right.png' %>
      </div>

    </div>

  </div>


  <%= render 'profile_bottom' %>

</main>

<main class="row g-0 position-relative window-holder">

  <div class="culture-left col position-relative h-100 text-center" style="width:50%; background: url(<%= asset_pack_path 'media/images/corp/culture/gt/bg-left.png'%>) ">
    <%= image_pack_tag 'media/images/corp/culture/gt/left1.png', style: 'margin-top: 260px;' %>
    <div class="w-100"></div>
    <%= image_pack_tag 'media/images/corp/culture/gt/left2.png', style: 'margin-top: 90px;' %>
    <div class="w-100"></div>
    <%= image_pack_tag 'media/images/corp/culture/gt/left3.png', style: 'margin-top: 112px;' %>

  </div>


  <div class="culture-right col position-relative h-100 text-center" style="width:50%; background: url(<%= asset_pack_path 'media/images/corp/culture/gt/bg-right.png'%>) ">
    <%= image_pack_tag 'media/images/corp/culture/gt/500.png', style: 'margin-top: 200px;' %>
    <div class="w-100"></div>
    <%= image_pack_tag 'media/images/corp/culture/gt/1.png', style: 'margin-top: 90px;' %>
    <div class="w-100"></div>
    <%= image_pack_tag 'media/images/corp/culture/gt/2.png', style: 'margin-top: 35px;' %>
    <div class="w-100"></div>
    <%= image_pack_tag 'media/images/corp/culture/gt/3.png', style: 'margin-top: 35px;' %>
  </div>

  <%= render 'profile_bottom' %>


</main>

<div class="innovation-result-page window-holder position-relative" style="">

  <div class="row g-0 position-relative ">
    <!-- main left -->
    <%= render 'innovation_left' %>

    <!-- main right -->
    <div class=" position-relative " style="width: 70%; ">

      <div class="row g-0" style='margin-top: 300px; width: 1750px; overflow: scroll;'>

        <div class="d-flex">
          <div class="">
            <%= image_pack_tag 'media/images/advantages/innovation/achievement/1.png', class: '' %>
          </div>
          <div class="">
            <%= image_pack_tag 'media/images/advantages/innovation/achievement/2.png', class: '' %>
          </div>
          <div class="">
            <%= link_to( advantages_innovation_achievement_more_path, html_options = {}) do %>
              <%= image_pack_tag 'media/images/advantages/innovation/achievement/3.png', class: '' %>
            <% end %>
          </div>
        </div>

      </div>

      <%= render 'innovation_navs', extra_link: {
        path: advantages_innovation_achievement_more_path,
        img_path: 'media/images/advantages/innovation/nav-achievement-more.png' } %>


    </div>

  </div>

</div>

<main class="window-holder position-relative" style="background: url(<%= asset_pack_path 'media/images/party/bg.png'%>) "  >
  <div class="w-100" style="height: 225px;"></div>

  <div class="d-flex">
    <!-- left content -->
    <div class=""  style="width:40%; padding-left: 67px;">
      <div class="row">
        <div class="col-12 text-center">
          <%= image_pack_tag 'media/images/party/nnl/lead/title.png', class: "" %>
        </div>
        <div class="col-12 text-center mt-5">
          <%= image_pack_tag 'media/images/party/nnl/lead/title-1.png', class: "tag active", data: {id: 1} %>
        </div>
        <div class="col-12 text-center mt-5">
          <%= image_pack_tag 'media/images/party/nnl/lead/title-2.png', class: "tag", data: {id: 2} %>
        </div>
      </div>
    </div>

    <!-- right content -->
    <div class="" style="width:60%;" >

      <div class="row g-0 mx-auto" style="width:100%; margin-top: 100px;">
        <!-- main content -->
        <div class="col-12 text-center position-relative" style=" " >

          <%= image_pack_tag 'media/images/party/nnl/lead/1-detail.png', class: "detail-content active", data: {id: 1} %>

          <div class="detail-content" data-id="2" style="width: 1126px; height: 524px; background: center no-repeat url(<%= asset_pack_path 'media/images/party/nnl/lead/2-detail-bg.png'%>) ">
            <div class="w-100" style="height: 120px;"></div>
            <div class="" style="width: 1020px; height: 380px; margin-left: 85px; overflow-x: scroll;">
              <%= image_pack_tag 'media/images/party/nnl/lead/2-detail-imgs.png' %>
            </div>

          </div>


        </div>

      </div>

    </div>

  </div>






  <%# render 'profile_bottom', return_path: corp_all_path %>

</main>

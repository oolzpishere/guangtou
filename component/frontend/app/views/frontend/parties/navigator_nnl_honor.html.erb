<main class="window-holder position-relative" style="background: url(<%= asset_pack_path 'media/images/party/bg.png'%>) "  >


  <div class="w-100" style="height: 225px;"></div>

  <div class="row g-0 mx-auto" style="width:90%;">
    <div class="col-12">
      <%= image_pack_tag 'media/images/party/nnl/honor/title.png', class: "" %>
    </div>
    <div class="col-12 position-relative" style="margin-top: 60px;">
      <div class="scroll-left-dark-arrows scroll-start-half"></div>
      <div class="scroll-right-dark-arrows scroll-end-half"></div>

      <div class="scroll-wrap slow-speed" style="overflow: scroll;">
        <%= image_pack_tag 'media/images/party/nnl/honor/imgs.png', class: "scroll-content" %>
      </div>

    </div>
  </div>


  <%# render 'profile_bottom', return_path: corp_all_path %>

</main>

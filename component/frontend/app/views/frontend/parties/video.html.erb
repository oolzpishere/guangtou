<main class="window-holder position-relative" style="background: url(<%= asset_pack_path 'media/images/party/second-bg.png'%>) "  >

  <%= render 'party_head' %>


  <div class="row g-0">
    <!-- render left navs -->
    <%= render 'left_navs', return_path: parties_path %>

    <!-- right content -->
    <div class="col" style="" >

      <div class="row mx-auto" style="width:90%; margin-top: 200px;">
        <!-- main content -->
        <div class="col-12 text-center" >
          <%= image_pack_tag 'media/images/party/video/text.png', class: "" %>

          <div id="" class="mt-3" style="">
            <!-- <i class="bi bi-x-lg position-absolute close-video" style="font-size: 2rem; color: white; bottom: 0; left: -2em;"></i> -->
            <!-- data-setup='{"controls": true}'> -->
            <video
                id="brand-video"
                class="video-js my-vjs-skin m-auto"
                width="1350"
                enableTouchActivity
                preload='auto'
                poster=<%= asset_pack_path 'media/images/party/video/cover.png' %>
                data-setup=''>
              <source src="/videos/strategy.mp4" type="video/mp4"></source>
              <p class="vjs-no-js">
                To view this video please enable JavaScript, and consider upgrading to a
                web browser that
              </p>
            </video>
          </div>

        </div>


      </div>

    </div>

  </div>






  <%# render 'profile_bottom', return_path: corp_all_path %>

</main>

<main class="window-holder position-relative" style="background: url(<%= asset_pack_path 'media/images/party/second-bg.png'%>) "  >

  <%= render 'party_head' %>


  <div class="row g-0">
    <!-- render left navs -->
    <div class="col-auto" style="width:30%;">
      <%= render 'left_navs' %>
    </div>


    <!-- right content -->
    <div class="col" style="width:70%;" >

      <div class="row g-0 mx-auto" style="width:100%; margin-top: 200px;">
        <!-- main content -->
        <div class="col-12 text-center position-relative" style="width: 1820px; height: 990px; overflow-y:scroll;" >


          <!-- first page -->
          <%= image_pack_tag 'media/images/party/lead/page1-title.png', class: "" %>
          <%= image_pack_tag 'media/images/party/lead/page1-img.png', class: "my-5" %>

          <!-- second page -->
          <%= image_pack_tag 'media/images/party/lead/page2-title.png', class: "mt-5" %>
          <%= image_pack_tag 'media/images/party/lead/page2-img.png', class: "mt-5" %>

          <!-- <div class="scroll-icon position-absolute" style="left: 50%; transform:translate(-50%,0); bottom:0;">
            <span></span>
          </div> -->

        </div>


      </div>

    </div>

  </div>






  <%# render 'profile_bottom', return_path: corp_all_path %>

</main>

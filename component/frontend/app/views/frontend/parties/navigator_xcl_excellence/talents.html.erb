<main class="window-holder position-relative" style="background: url(<%= asset_pack_path 'media/images/corp/strategy/bg.png'%>) "  >

  <div class="w-100" style="height:290px;"></div>
  <div class="row g-0" style="width: 90%; margin-left: 8%; ">

    <div class="col-12">
      <%= image_pack_tag 'media/images/party/xcl/excellence/talents/title.png', class: "" %>

    </div>

    <div class="row g-0 " style="margin-top: -30px;">
      <!-- Slider main container -->
      <div class="swiper-container talents-swiper">
        <!-- Additional required wrapper -->
        <div class="swiper-wrapper">
          <!-- Slides, talents with detail -->
          <% (1..1).each do |i| %>
            <div class="swiper-slide talent-slide talent-slide-with-detail text-center " style="">
              <!-- .expanded -->
              <!-- img: width: 515px; height: 559px; -->
              <div class="row g-0 align-items-center h-100">
                <div class="col position-relative">
                  <%= image_pack_tag "media/images/party/xcl/excellence/talents/#{i}.png", class: "talent-img" %>
                  <%= image_pack_tag "media/images/party/xcl/excellence/talents/more.png", class: "talent-img position-absolute", style: 'top: 510px; left: 50%; margin-left: -81px;' %>
                </div>
                <div class="col-auto talent-detail" style="width: 1237px; height: 836px; background: url(<%= asset_pack_path "media/images/party/xcl/excellence/talents/0#{i}.png"%>) " >
                  <div class="position-relative" style="margin-top: 205px; margin-left: 180px; ">
                    <video
                        id="party-talent-cxy-video"
                        class="video-js my-vjs-skin"
                        width="950"
                        enableTouchActivity
                        poster=<%= asset_pack_path 'media/images/party/xcl/excellence/talents/poster-cxy.jpg' %>
                        data-setup=''>
                      <source src="/videos/parties/talents/cxy.mp4" type="video/mp4"></source>
                    </video>
                  </div>

                </div>

              </div>

            </div>
          <% end %>

          <!-- Slides, talents  -->
          <% (2..2).each do |i| %>
            <div class="swiper-slide talent-slide text-center" style="">
              <div class="row g-0 align-items-center h-100">
                <div class="col ">
                  <%= image_pack_tag "media/images/party/xcl/excellence/talents/#{i}.png" %>
                </div>
              </div>
            </div>
          <% end %>

          <% (3..3).each do |i| %>
            <div class="swiper-slide talent-slide talent-slide-with-detail text-center " style="">
              <!-- .expanded -->
              <!-- img: width: 515px; height: 559px; -->

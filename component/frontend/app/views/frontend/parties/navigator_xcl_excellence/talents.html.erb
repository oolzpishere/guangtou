                <div class="col position-relative">
                  <%= image_pack_tag "media/images/party/xcl/excellence/talents/#{i}.png", class: "talent-img" %>
                  <%= image_pack_tag "media/images/party/xcl/excellence/talents/more.png", class: "talent-img position-absolute", style: 'top: 510px; left: 50%; margin-left: -81px;' %>
                </div>
                <div class="col-auto talent-detail" style="width: 1237px; height: 836px; background: url(<%= asset_pack_path "media/images/party/xcl/excellence/talents/0#{i}.png"%>) ">
